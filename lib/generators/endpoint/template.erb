<% docs.each do |doc| %>
def <%= doc.name %>(params={}<% if doc.has_body -%>, options={}<% end -%>)
<% if doc.param != nil -%>
  <%= doc.param %>
<% end -%>
  path = <%= doc.url %>
<% if doc.query != nil -%>
  path += <%= doc.query %>
<% end -%>
  request(:<%= doc.verb %>, path<% if doc.has_body -%>, options<% end -%>)
end
<% end %>
